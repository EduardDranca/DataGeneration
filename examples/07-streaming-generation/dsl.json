{
    "reference_data": {
        "count": 5,
        "tags": ["reference"],
        "item": {
            "id": {"gen": "uuid"},
            "category": {"gen": "choice", "options": ["A", "B", "C", "D", "E"]},
            "name": {"gen": "company.name"}
        }
    },
    "large_dataset": {
        "count": 1000,
        "item": {
            "id": {"gen": "uuid"},
            "timestamp": {"gen": "date"},
            "value": {"gen": "float", "decimals": 2, "min": 0.0, "max": 1000.0},
            "categoryId": {"ref": "reference_data[*].id"},
            "status": {"gen": "choice", "options": ["active", "inactive", "pending"]},
            "metadata": {
                "source": {"gen": "choice", "options": ["api", "batch", "stream"]},
                "priority": {"gen": "number", "min": 1, "max": 10}
            }
        }
    }
}
